module Spotted
  module Models
    type audiobook_retrieve_response =
      { chapters: Spotted::Models::AudiobookRetrieveResponse::Chapters }

    class AudiobookRetrieveResponse < Spotted::Models::AudiobookBase
      def chapters: -> Spotted::Models::AudiobookRetrieveResponse::Chapters

      def chapters=: (
        Spotted::Models::AudiobookRetrieveResponse::Chapters _
      ) -> Spotted::Models::AudiobookRetrieveResponse::Chapters

      def initialize: (
        chapters: Spotted::Models::AudiobookRetrieveResponse::Chapters
      ) -> void

      def to_hash: -> {
        chapters: Spotted::Models::AudiobookRetrieveResponse::Chapters
      }

      type chapters =
        {
          href: String,
          limit: Integer,
          next_: String?,
          offset: Integer,
          previous: String?,
          total: Integer,
          items: ::Array[Spotted::SimplifiedChapterObject],
          published: bool
        }

      class Chapters < Spotted::Internal::Type::BaseModel
        attr_accessor href: String

        attr_accessor limit: Integer

        attr_accessor next_: String?

        attr_accessor offset: Integer

        attr_accessor previous: String?

        attr_accessor total: Integer

        attr_reader items: ::Array[Spotted::SimplifiedChapterObject]?

        def items=: (
          ::Array[Spotted::SimplifiedChapterObject]
        ) -> ::Array[Spotted::SimplifiedChapterObject]

        attr_reader published: bool?

        def published=: (bool) -> bool

        def initialize: (
          href: String,
          limit: Integer,
          next_: String?,
          offset: Integer,
          previous: String?,
          total: Integer,
          ?items: ::Array[Spotted::SimplifiedChapterObject],
          ?published: bool
        ) -> void

        def to_hash: -> {
          href: String,
          limit: Integer,
          next_: String?,
          offset: Integer,
          previous: String?,
          total: Integer,
          items: ::Array[Spotted::SimplifiedChapterObject],
          published: bool
        }
      end
    end
  end
end
