module Spotted
  module Models
    type author_object = { name: String, published: bool }

    class AuthorObject < Spotted::Internal::Type::BaseModel
      attr_reader name: String?

      def name=: (String) -> String

      attr_reader published: bool?

      def published=: (bool) -> bool

      def initialize: (?name: String, ?published: bool) -> void

      def to_hash: -> { name: String, published: bool }
    end
  end
end
